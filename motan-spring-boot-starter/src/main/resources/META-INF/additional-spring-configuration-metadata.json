{
  "properties": [
    {
      "name": "spring.motan.scan.package",
      "type": "java.lang.String"
    },
    {
      "name": "spring.motan.protocol.name",
      "type": "java.lang.String",
      "values": [
        {
          "value": "motan"
        },
        {
          "value": "motan2"
        }
      ]
    },
    {
      "name": "spring.motan.protocol.maxContentLength",
      "type": "java.lang.Integer",
      "defaultValue": 1048576
    },
    {
      "name": "spring.motan.protocol.haStrategy",
      "type": "java.lang.String",
      "values": [
        {
          "value": "failfast"
        },
        {
          "value": "failover"
        }
      ],
      "defaultValue": "failfast"
    },
    {
      "name": "spring.motan.protocol.loadbalance",
      "type": "java.lang.String",
      "values": [
        {
          "value": "roundrobin"
        },
        {
          "value": "random"
        },
        {
          "value": "localFirst"
        },
        {
          "value": "consistent"
        },
        {
          "value": "configurableWeight"
        },
        {
          "value": "activeWeight"
        }
      ],
      "defaultValue": "roundrobin"
    },
    {
      "name": "spring.motan.registry.regProtocol",
      "type": "java.lang.String",
      "values": [
        {
          "value": "zookeeper"
        },
        {
          "value": "consul"
        },
        {
          "value": "direct"
        }
      ],
      "defaultValue": "zookeeper"
    },
    {
      "name": "spring.motan.registry.address",
      "type": "java.lang.String",
      "description": "motan registry address.",
      "defaultValue": "127.0.0.1:2181"
    },
    {
      "name": "spring.motan.service.group",
      "type": "java.lang.String",
      "description": "motan service group.",
      "defaultValue": "default"
    },
    {
      "name": "spring.motan.service.check",
      "type": "java.lang.Boolean",
      "description": "motan service check exists.",
      "defaultValue": false
    },
    {
      "name": "spring.motan.service.accessLog",
      "type": "java.lang.Boolean",
      "description": "motan service print access log.",
      "defaultValue": false
    },
    {
      "name": "spring.motan.service.shareChannel",
      "type": "java.lang.Boolean",
      "description": "motan service share channel.",
      "defaultValue": true
    },
    {
      "name": "spring.motan.service.module",
      "type": "java.lang.String",
      "description": "motan service module.",
      "defaultValue": "default"
    },
    {
      "name": "spring.motan.service.application",
      "type": "java.lang.String",
      "description": "motan service application name.",
      "defaultValue": "default-server"
    },
    {
      "name": "spring.motan.service.export",
      "type": "java.lang.String",
      "description": "motan service export.",
      "defaultValue": "motanProtocol:8002"
    },
    {
      "name": "spring.motan.referer.group",
      "type": "java.lang.String",
      "description": "motan referer group.",
      "defaultValue": "default"
    },
    {
      "name": "spring.motan.referer.check",
      "type": "java.lang.Boolean",
      "description": "motan referer check exists.",
      "defaultValue": false
    },
    {
      "name": "spring.motan.referer.accessLog",
      "type": "java.lang.Boolean",
      "description": "motan referer print access log.",
      "defaultValue": false
    },
    {
      "name": "spring.motan.referer.shareChannel",
      "type": "java.lang.Boolean",
      "description": "motan referer share channel.",
      "defaultValue": true
    },
    {
      "name": "spring.motan.referer.module",
      "type": "java.lang.String",
      "description": "motan referer module.",
      "defaultValue": "default"
    },
    {
      "name": "spring.motan.referer.application",
      "type": "java.lang.String",
      "description": "motan referer application name.",
      "defaultValue": "default-client"
    },
    {
      "name": "spring.motan.referer.retries",
      "type": "java.lang.Integer",
      "description": "motan referer retries.",
      "defaultValue": 2
    },
    {
      "name": "spring.motan.referer.throwException",
      "type": "java.lang.Boolean",
      "description": "motan server service throw exception.",
      "defaultValue": true
    }
  ]
}